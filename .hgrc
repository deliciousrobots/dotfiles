[ui]
username = Steve Goss <sgoss@kavi.com>
merge = internal:merge

[trusted]
users = web
group = web

[extensions]
# Get rid of annoying "failed to import extension hgext/hct" errors:
hgext/hct=!
# enable glog command: http://mercurial.selenic.com/wiki/GraphlogExtension
hgext.graphlog=
# enable fetch command: http://mercurial.selenic.com/wiki/FetchExtension
hgext.fetch=
# enable transplant command: http://mercurial.selenic.com/wiki/TransplantExtension
transplant=
# colorized diffs
hgext.color=
# enable MqExtension: http://mercurial.selenic.com/wiki/MqExtension
hgext.mq =

hgext.shelve = ~/plugins/hgshelve/hgshelve.py

# enable bookmarks extension
hgext.bookmarks=
